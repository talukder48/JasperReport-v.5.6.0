	<queryString>
		<![CDATA[select nothi_no,
 decode(p.month_code,'1','January','2','February','3','March','4','April','5','May','6','June','7','July','8','August','9','September','10','October','11','November','December') Month_name,
       '' enothi,

         (select i.benificiaryname ||':-'||decode(i.relationtype,'W','Wife','D','Daughter','S','Son','H','Husband')
         from pen_inheritance i where i.nothi_num=p.nothi_no
         and i.successor_sl=p.tran_sl)successor_name,
       (SELECT c.pension_amt FROM pen_calculation_data c where c.nothi_num=p.nothi_no) ActualPension ,
       (SELECT c.medical_amt FROM pen_calculation_data c where c.nothi_num=p.nothi_no)ActualMedical ,
       (SELECT i.pension_pct FROM pen_inheritance i where i.nothi_num=p.nothi_no and i.successor_sl=p.tran_sl)ActualPen_pct,
       emp_id ,
       emp_name,
       'Single' district,
       designation,
       pension_basic,
       pension_arrear,
       pen_medical_alw,
       pen_medical_arear,
       pen_bonus+pen_bonus_arear as pen_bonus,
       p.dearness+p.arr_dearness as dearness,
       p.noborsho,
       others_alw,
       hb_ded_amount,
       com_ded_amt,
       mot_ded_amt,
       others_ded_amt,
       revnue,
       gross_amount,
       total_deduction,
       net_payment,
       bank_name,
       branch_name,
       bank_account,
       decode((SELECT x.pensioner_type FROM pen_employee x where x.nothi_num=p.nothi_no),'S','100 %Surrender','M','Monthly Pensioner','R','Replaced') pentionType
  from pen_transation_details p
 where p.entity_num = $P{P_ENTITY_NUM}
   and p.year = $P{P_YEAR}
   and p.month_code = $P{P_MONTH}
 and  decode(branch_location,'DM','D','Dhaka','D','O') like $P{PensionBranchLocation}
 and  activationtype like $P{ActivationType}
order by to_number(substr(NOTHI_NO,1,3))]]>
	</queryString>
