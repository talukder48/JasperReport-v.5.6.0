	<queryString>
		<![CDATA[SELECT ROWNUM SL_NO,
       T.EMP_ID,
       TRIM(T.SAL_MONTH) SAL_MONTH,
       T.SAL_YEAR,
       (SELECT E.EMP_NAME FROM PRMS_EMPLOYEE E WHERE E.EMP_ID = T.EMP_ID) EMP_NAME,
       (SELECT DECODE(E.TIN_NO, NULL, 'N/A', E.TIN_NO) TIN_NO
          FROM PRMS_EMPLOYEE E
         WHERE E.EMP_ID = T.EMP_ID) TIN,
       (SELECT S.BANK_ACC FROM PRMS_EMP_SAL S WHERE S.EMP_ID = T.EMP_ID) BANK_ACC,
       (SELECT S.DESIG FROM PRMS_EMP_SAL S WHERE S.EMP_ID = T.EMP_ID) DESIGNATION,
       (SELECT D.DEPT_NAME FROM PRMS_BRN_DEPARTMENT D WHERE D.BRN_CODE = T.BRANCH_CODE AND D.DEPT_CODE = (SELECT S.EMP_DEPT_CODE FROM PRMS_EMP_SAL S WHERE S.EMP_ID = T.EMP_ID)) DEPT,
       T.BASIC_PAY,
       NVL(T.ARREAR_BASIC, 0) ARREAR_BASIC,
       NVL(T.HOUSE_RENT_ALLOWANCE, 0) HOUSE_RENT_ALLOWANCE,
       NVL(T.ARREAR, 0) ARREAR,
       NVL(T.TEL_ALLOWANCE, 0) TEL_ALLOWANCE,
       NVL(T.TRANS_ALLOWANCE, 0) TRANS_ALLOWANCE,
       NVL(T.EDU_ALLOWANCE, 0) EDU_ALLOWANCE,
       NVL(T.WASH_ALLOWANCE, 0) WASH_ALLOWANCE,
       NVL(T.ENTERTAINMENT, 0) ENTERTAINMENT,
       NVL(T.DOMESTIC_ALLOWANCE, 0) DOMESTIC_ALLOWANCE,
       '0.00' DEARNESS,
       T.OTHER_ALLOWANCE,
       T.PENSION_ALLOWANCE,
       T.MEDICAL_ALLOWANCE,
       T.TOT_SAL_ALLOWANCE,
       T.GROSS_PAY_AMT,
       T.PFADV_DEDUC,
       T.PF_DEDUCTION,
       T.PFADV_ARREAR_DEDUC,
       (SELECT S.PF_DEDUCTION_PCT
          FROM PRMS_EMP_SAL S
         WHERE S.EMP_ID = T.EMP_ID) PF_PCT,
       T.HBADV_DEDUC,
       T.HBADV_DEDUC_PERCENT,
       T.HBADV_ARREAR_DEDUC,
       T.WELFARE_DEDUC,
       T.MCYCLE_DEDUC,
       T.BICYCLE_DEDUC,
       T.COMP_DEDUC,
       T.CARFARE_DEDUC,
       T.CARUSE_DEDUC,
       T.GAS_BILL,
       T.WATER_BILL,
       T.ELECTRICITY_BILL,
       T.NEWS_PAPER_DEDUC,
       T.TEL_EXCESS_BILL,
       T.GEN_INSURENCE,
       NVL(T.SP_DEDUC,0) SP_DEDUC,
       T.OTHER_DEDUC,
       T.REVENUE_DEDUC,
       T.SP_DEDUC,
       NVL(T.HILL_ALLWNC,0) HILL_ALLWNC,
       T.PENSION_DEDUC,
       NVL(T.NET_DED_AMT, 0) NET_DED_AMT,
       NVL(T.NET_PAY_AMT, 0) NET_PAY_AMT,
       INCOME_TAX,
       INCOME_TAX_ARR,
       (SELECT PRMS_AMOUNT_TO_WORDS(T.NET_PAY_AMT)||' Only.' AMOUNT_WORD FROM DUAL) NET_PAY_AMT_IN_WORD,
(SELECT S.DESIG_CODE FROM PRMS_EMP_SAL S WHERE S.EMP_ID = T.EMP_ID) EMP_DSIG_CODE,
       (SELECT S.DESIG_SENIORITY_CODE
          FROM PRMS_EMP_SAL S
         WHERE S.EMP_ID = T.EMP_ID) EMP_SENIORITY,
(SELECT DECODE(D.DESIGNATION_CATEGORY, '1', 'OFFICER', 'STAFF')
          FROM PRMS_DESIGNATION D
         WHERE D.DESIGNATION_CODE =
               (SELECT S.DESIG_CODE
                  FROM PRMS_EMP_SAL S
                 WHERE S.EMP_ID = T.EMP_ID)) EMP_TYPE
  FROM PRMS_TRANSACTION T
 WHERE T.ENTITY_NUMBER = $P{P_ENTITY_NUM}
   AND T.BRANCH_CODE = $P{P_BRANCH}
   AND T.EMP_ID =$P{P_EMP_ID}
   AND T.SAL_YEAR = $P{P_YEAR}
   AND T.MONTH_CODE = $P{P_MONTH}
ORDER BY EMP_TYPE,EMP_DSIG_CODE,EMP_SENIORITY]]>
	</queryString>
