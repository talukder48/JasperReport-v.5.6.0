	<queryString>
		<![CDATA[SELECT
       T.EMP_ID,
       (SELECT E.EMP_NAME FROM PRMS_EMPLOYEE E WHERE E.EMP_ID = T.EMP_ID) EMP_NAME,
       (SELECT DECODE(E.TIN_NO, NULL, 'N/A', E.TIN_NO) TIN_NO
          FROM PRMS_EMPLOYEE E
         WHERE E.EMP_ID = T.EMP_ID) TIN,
       (SELECT S.BANK_ACC FROM PRMS_EMP_SAL S WHERE S.EMP_ID = T.EMP_ID) BANK_ACC,
       (SELECT S.DESIG FROM PRMS_EMP_SAL S WHERE S.EMP_ID = T.EMP_ID) DESIGNATION,
       (SELECT TO_CHAR(ADD_MONTHS(DOB, 60 * 12) - 1)
          FROM PRMS_EMPLOYEE
         WHERE EMP_ID = T.EMP_ID) LPR_LAST_DATE,

       (SELECT S.PF_DEDUCTION_PCT
          FROM PRMS_EMP_SAL S
         WHERE S.EMP_ID = T.EMP_ID) PF_PCT,

       (SELECT S.DESIG_CODE FROM PRMS_EMP_SAL S WHERE S.EMP_ID = T.EMP_ID) EMP_DSIG_CODE,
       (SELECT S.DESIG_SENIORITY_CODE
          FROM PRMS_EMP_SAL S
         WHERE S.EMP_ID = T.EMP_ID) EMP_SENIORITY,
       (SELECT DECODE(D.DESIGNATION_CATEGORY, '1', 'OFFICER', 'STAFF')
          FROM PRMS_DESIGNATION D
         WHERE D.DESIGNATION_CODE =
               (SELECT S.DESIG_CODE
                  FROM PRMS_EMP_SAL S
                 WHERE S.EMP_ID = T.EMP_ID)) EMP_TYPE

      ,0 HBADV_DEDUC_PERCENT,
       sum(BASIC_PAY) BASIC_PAY,
       sum(ARREAR_BASIC) ARREAR_BASIC,
       sum(HOUSE_RENT_ALLOWANCE) HOUSE_RENT_ALLOWANCE,
       sum(ARREAR) ARREAR,
       sum(TEL_ALLOWANCE) TEL_ALLOWANCE,
       sum(TRANS_ALLOWANCE) TRANS_ALLOWANCE,
       sum(EDU_ALLOWANCE) EDU_ALLOWANCE,
       sum(WASH_ALLOWANCE) WASH_ALLOWANCE,
       sum(ENTERTAINMENT) ENTERTAINMENT,
       sum(DOMESTIC_ALLOWANCE) DOMESTIC_ALLOWANCE,
       sum(HILL_ALLWNC) HILL_ALLWNC,
       sum(OTHER_ALLOWANCE) OTHER_ALLOWANCE,
       sum(PENSION_ALLOWANCE) PENSION_ALLOWANCE,
       sum(MEDICAL_ALLOWANCE) MEDICAL_ALLOWANCE,
       sum(TOT_SAL_ALLOWANCE) TOT_SAL_ALLOWANCE,
       sum(GROSS_PAY_AMT) GROSS_PAY_AMT,
       sum(PFADV_DEDUC) PFADV_DEDUC,
       sum(PF_DEDUCTION) PF_DEDUCTION,
       sum(PFADV_ARREAR_DEDUC) PFADV_ARREAR_DEDUC,
       sum(HBADV_DEDUC) HBADV_DEDUC,
       sum(HBADV_ARREAR_DEDUC) HBADV_ARREAR_DEDUC,
       sum(WELFARE_DEDUC) WELFARE_DEDUC,
       sum(MCYCLE_DEDUC) MCYCLE_DEDUC,
       sum(BICYCLE_DEDUC) BICYCLE_DEDUC,
       sum(COMP_DEDUC) COMP_DEDUC,
       sum(CARFARE_DEDUC) CARFARE_DEDUC,
       sum(CARUSE_DEDUC) CARUSE_DEDUC,
       sum(GAS_BILL) GAS_BILL,
       sum(WATER_BILL) WATER_BILL,
       sum(ELECTRICITY_BILL) ELECTRICITY_BILL,
       sum(NEWS_PAPER_DEDUC) NEWS_PAPER_DEDUC,
       sum(TEL_EXCESS_BILL) TEL_EXCESS_BILL,
       sum(GEN_INSURENCE) GEN_INSURENCE,
       sum(OTHER_DEDUC) OTHER_DEDUC,
       sum(REVENUE_DEDUC) REVENUE_DEDUC,
       sum(SP_DEDUC) SP_DEDUC,
       sum(PENSION_DEDUC) PENSION_DEDUC,
       sum(NET_DED_AMT) NET_DED_AMT,
       sum(NET_PAY_AMT) NET_PAY_AMT,
       sum(INCOME_TAX) INCOME_TAX,
       sum(INCOME_TAX_ARR) INCOME_TAX_ARR

  FROM
  (select *from prms_transaction t
 WHERE T.ENTITY_NUMBER = 1
   AND T.BRANCH_CODE = '9999'
   AND T.SAL_YEAR = 2019
    and t.emp_id in('675')
   and t.month_code>11

   union

   select *from prms_transaction t
 WHERE T.ENTITY_NUMBER = 1
   AND T.BRANCH_CODE = '9999'
   AND T.SAL_YEAR = 2020
   and t.emp_id in('675')

   and t.month_code<7

   ) t
 group by  EMP_ID
order by EMP_DSIG_CODE]]>
