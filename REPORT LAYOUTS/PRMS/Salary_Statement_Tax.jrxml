	<queryString>
		<![CDATA[SELECT *
  FROM (SELECT T.BRANCH_CODE,
               T.EMP_ID,
(SELECT DECODE(D.DESIGNATION_CATEGORY, '1', 'OFFICER', 'STAFF')
                  FROM PRMS_DESIGNATION D
                 WHERE D.DESIGNATION_CODE =
                       (SELECT S.DESIG_CODE
                          FROM PRMS_EMP_SAL S
                         WHERE S.EMP_ID = T.EMP_ID)) EMP_TYPE,
               (SELECT S.DESIG_CODE
                  FROM PRMS_EMP_SAL S
                 WHERE S.EMP_ID = T.EMP_ID) EMP_DSIG_CODE,
               (SELECT S.DESIG_SENIORITY_CODE
                  FROM PRMS_EMP_SAL S
                 WHERE S.EMP_ID = T.EMP_ID) EMP_SENIORITY,
               (SELECT S.DESIG FROM PRMS_EMP_SAL S WHERE S.EMP_ID = T.EMP_ID) DESIGNATION,
               (SELECT E.EMP_NAME
                  FROM PRMS_EMPLOYEE E
                 WHERE E.EMP_ID = T.EMP_ID) EMP_NAME,
               SUBSTR(T.SAL_MONTH, 1, 3) || '-' || T.SAL_YEAR MONTH_YEAR,
               T.SAL_YEAR,
               T.MONTH_CODE,
                NVL(T.BASIC_PAY + T.ARREAR_BASIC, 0) +NVL(PKG_PRMS.FN_MANUAL_SAL_DATA(T.ENTITY_NUMBER,T.BRANCH_CODE,T.EMP_ID,T.SAL_YEAR,T.MONTH_CODE),0) basic_pay,
               NVL(T.DOMESTIC_ALLOWANCE, 0) DOMESTIC_ALLOWANCE,
               NVL(T.ENTERTAINMENT, 0) ENTERTAINMENT,
               NVL(T.MEDICAL_ALLOWANCE, 0) MEDICAL_ALLOWANCE,
               NVL(T.HOUSE_RENT_ALLOWANCE, 0) HOUSE_RENT_ALLOWANCE,
               NVL(T.EDU_ALLOWANCE, 0) EDU_ALLOWANCE,
               NVL(T.PF_DEDUCTION, 0)+NVL(PKG_PRMS.FN_MANUAL_SAL_DATA_pf(T.ENTITY_NUMBER,T.BRANCH_CODE,T.EMP_ID,T.SAL_YEAR,T.MONTH_CODE),0) PF_DEDUCTION,
               NVL(T.WELFARE_DEDUC, 0) WELFARE_DEDUC,
       NVL(T.INCOME_TAX + T.INCOME_TAX_ARR, 0) INCOME_TAX,
               NVL(T.TEL_ALLOWANCE, 0) TEL_ALLOWANCE,
               NVL(T.DEARNESS_ALLOWANCE, 0) DEARNESS_ALLOWANCE,
               NVL(T.GEN_INSURENCE, 0) GEN_INSURENCE,

               (SELECT B.BONUS_TYPE FROM PRMS_BONUS_TRANSACTION B
                       WHERE B.ENTITY_NUM = T.ENTITY_NUMBER
                         AND B.BONUS_TYPE IN ('EIDFT', 'EIDAH', 'DURGA', 'NEWYR', 'INCTV')
                         AND B.SAL_YEAR = T.SAL_YEAR
                         AND B.BRANCH_CODE=T.BRANCH_CODE
                         AND B.EMP_ID = T.EMP_ID
                         AND B.MONTH_CODE = T.MONTH_CODE) BONUS_TYPE,
               (SELECT B.BONUS_AMOUNT FROM PRMS_BONUS_TRANSACTION B
                       WHERE B.ENTITY_NUM = T.ENTITY_NUMBER
                         AND B.BONUS_TYPE IN ('EIDFT', 'EIDAH', 'DURGA', 'NEWYR', 'INCTV')
                         AND B.SAL_YEAR = T.SAL_YEAR
                         AND B.BRANCH_CODE=T.BRANCH_CODE
                         AND B.EMP_ID = T.EMP_ID
                         AND B.MONTH_CODE = T.MONTH_CODE) BONUS_AMOUNT
          FROM PRMS_TRANSACTION T
         WHERE T.ENTITY_NUMBER = $P{P_ENTITY_NUM}
           AND T.BRANCH_CODE like  $P{P_BRANCH}||'%'
           AND T.SAL_YEAR = $P{P_FIN_YEAR}
  AND (T.EMP_ID = $P{P_EMP_ID} OR $P{P_EMP_ID} IS NULL)
           AND T.MONTH_CODE > 6
        UNION
        SELECT T.BRANCH_CODE,
               T.EMP_ID,
(SELECT DECODE(D.DESIGNATION_CATEGORY, '1', 'OFFICER', 'STAFF')
                  FROM PRMS_DESIGNATION D
                 WHERE D.DESIGNATION_CODE =
                       (SELECT S.DESIG_CODE
                          FROM PRMS_EMP_SAL S
                         WHERE S.EMP_ID = T.EMP_ID)) EMP_TYPE,
               (SELECT S.DESIG_CODE
                  FROM PRMS_EMP_SAL S
                 WHERE S.EMP_ID = T.EMP_ID) EMP_DSIG_CODE,
               (SELECT S.DESIG_SENIORITY_CODE
                  FROM PRMS_EMP_SAL S
                 WHERE S.EMP_ID = T.EMP_ID) EMP_SENIORITY,
               (SELECT S.DESIG FROM PRMS_EMP_SAL S WHERE S.EMP_ID = T.EMP_ID) DESIGNATION,
               (SELECT E.EMP_NAME
                  FROM PRMS_EMPLOYEE E
                 WHERE E.EMP_ID = T.EMP_ID) EMP_NAME,
               SUBSTR(T.SAL_MONTH, 1, 3) || '-' || T.SAL_YEAR MONTH_YEAR,
               T.SAL_YEAR,
               T.MONTH_CODE,
               NVL(T.BASIC_PAY + T.ARREAR_BASIC, 0) +NVL(PKG_PRMS.FN_MANUAL_SAL_DATA(T.ENTITY_NUMBER,T.BRANCH_CODE,T.EMP_ID,T.SAL_YEAR,T.MONTH_CODE),0) basic_pay,
               NVL(T.DOMESTIC_ALLOWANCE, 0) DOMESTIC_ALLOWANCE,
               NVL(T.ENTERTAINMENT, 0) ENTERTAINMENT,
               NVL(T.MEDICAL_ALLOWANCE, 0) MEDICAL_ALLOWANCE,
               NVL(T.HOUSE_RENT_ALLOWANCE, 0) HOUSE_RENT_ALLOWANCE,
               NVL(T.EDU_ALLOWANCE, 0) EDU_ALLOWANCE,
               NVL(T.PF_DEDUCTION, 0)+NVL(PKG_PRMS.FN_MANUAL_SAL_DATA_pf(T.ENTITY_NUMBER,T.BRANCH_CODE,T.EMP_ID,T.SAL_YEAR,T.MONTH_CODE),0) PF_DEDUCTION,
               NVL(T.WELFARE_DEDUC, 0) WELFARE_DEDUC,
       NVL(T.INCOME_TAX + T.INCOME_TAX_ARR, 0) INCOME_TAX,
               NVL(T.TEL_ALLOWANCE, 0) TEL_ALLOWANCE,
               NVL(T.DEARNESS_ALLOWANCE, 0) DEARNESS_ALLOWANCE,
               NVL(T.GEN_INSURENCE, 0) GEN_INSURENCE,

               (SELECT B.BONUS_TYPE FROM PRMS_BONUS_TRANSACTION B
                       WHERE B.ENTITY_NUM = T.ENTITY_NUMBER
                         AND B.BONUS_TYPE IN ('EIDFT', 'EIDAH', 'DURGA', 'NEWYR', 'INCTV')
                         AND B.SAL_YEAR = T.SAL_YEAR
                         AND B.BRANCH_CODE like $P{P_BRANCH}||'%'
                         AND B.EMP_ID = T.EMP_ID
                         AND B.MONTH_CODE = T.MONTH_CODE) BONUS_TYPE,
               (SELECT B.BONUS_AMOUNT FROM PRMS_BONUS_TRANSACTION B
                       WHERE B.ENTITY_NUM = T.ENTITY_NUMBER
                         AND B.BONUS_TYPE IN ('EIDFT', 'EIDAH', 'DURGA', 'NEWYR', 'INCTV')
                         AND B.SAL_YEAR = T.SAL_YEAR
                         AND B.BRANCH_CODE like  $P{P_BRANCH}||'%'
                         AND B.EMP_ID = T.EMP_ID
                         AND B.MONTH_CODE = T.MONTH_CODE) BONUS_AMOUNT
          FROM PRMS_TRANSACTION T
         WHERE T.ENTITY_NUMBER = $P{P_ENTITY_NUM}
            AND T.BRANCH_CODE like  $P{P_BRANCH}||'%'
           AND T.SAL_YEAR = $P{P_FIN_YEAR2}
  AND (T.EMP_ID = $P{P_EMP_ID} OR $P{P_EMP_ID} IS NULL)
           AND T.MONTH_CODE < 7)
 ORDER BY EMP_DSIG_CODE, EMP_ID, SAL_YEAR, MONTH_CODE]]>
	</queryString>
