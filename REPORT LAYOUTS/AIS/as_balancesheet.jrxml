       GROUP_CODE,
       PARTICULAR_CODE,
       PARTICULAR_NAME,
       PAR_PARTICULAR_CODE,
       VISIBILITY,
       CALCULATIONTYPE,
       case group_type
         when 'A' then
          BALANCE * (-1)
         else
          BALANCE
       end BALANCE,
       case group_type
         when 'A' then
          Sum_Balance * (-1)
         else
          Sum_Balance
       end Sum_Balance

  from table(pkg_gas_report.fn_get_balancesheet($P{P_BRANCH},$P{P_fin_year}))
