	<queryString>
		<![CDATA[select d.*, (select m.brn_name from prms_mbranch m where m.brn_code=d.branch_code)branch_name, (select a.borrower_name from loan_account a where a.loc_code=d.branch_code and a.loan_code=d.loan_code) borrowerName,
       (select p.purpose_desc
          from as_purpose_param p
         where p.purpose_code = d.purpose_code) Purpose_desc
  from api_transaction_data d
 where d.branch_code like $P{P_branch_code}
 and to_date(d.txn_date) between $P{p_fromDate} and $P{p_toDate}
 order by d.branch_code,to_date(d.txn_date)]]>
